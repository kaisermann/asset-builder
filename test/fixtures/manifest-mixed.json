{
	"config": {},
	"resources": {
		"scripts": {
			"pattern": "*.js",
			"directory": "scripts",
			"assets": {
				"app.js": {
					"files": [
						"scripts/**/*",
						"scripts/main.js"
					],
					"main": true,
					"vendor": []
				},
				"home.js": {
					"files": "scripts/home.js",
					"bower": [
						"slick.js"
					]
				},
				"jquery.js": {
					"bower": [
						"jquery"
					]
				},
				"modernizr.js": {
					"bower": [
						"modernizr.js"
					]
				}
			}
		},
		"styles": {
			"pattern": "*.{css,styl,less,scss}",
			"directory": "styles",
			"assets": {
				"app.css": {
					"files": "styles/main.less",
					"main": true
				}
			}
		}
	}
}
